{
  "launcherType": "bin",
  "config": {
    "executable": "$( locres apps/nuke/$UVER_NUKE_VERSION )/bin/$UCORE_OS/$UCORE_OS_VERSION/Nuke*"
  },
  "env": {
    "unset": [
      "QT_PLUGIN_PATH"
    ],
    "override": {
      "NUKE_TEMP_DIR": "$UMEDIA_TEMP_LOCAL_DIR"
    }
  },
  "addons": {
    "basetools": {
      "env": {
        "prepend" : {
          "PYTHONPATH": [
            "$( locres tools/base/$UVER_BASETOOLS_VERSION )/lib"
          ]
        }
      }
    },
    "neatvideo":{
      "env":{
        "prepend": {
          "OFX_PLUGIN_PATH": [
            "$( locres plugins/nuke/$(majorver $UVER_NUKE_VERSION)/neatVideo/$UVER_NEATVIDEO_VERSION )/bin/$UCORE_OS/$UCORE_OS_VERSION"
          ]
        }
      }
    },
    "eddy":{
      "env":{
        "prepend": {
          "NUKE_PATH": [
            "$( locres plugins/nuke/$(majorver $UVER_NUKE_VERSION)/eddy/$UVER_EDDY_VERSION )/bin/$UCORE_OS"
          ]
        }
      }
    }
  }
}
