{
  "launcherType": "bin",
  "config": {
    "executable": "$( locres apps/qbs/$UVER_QBS_VERSION )/bin/$UCORE_OS/$UCORE_OS_VERSION/bin/qbs"
  },
  "env": {
    "prepend": {
      "LD_LIBRARY_PATH": [
        "$( locres apps/qbs/$UVER_QBS_VERSION )/bin/$UCORE_OS/$UCORE_OS_VERSION/lib",
        "$( locres apps/gcc/$UVER_GCC_VERSION )/bin/$UCORE_OS/$UCORE_OS_VERSION/lib64",
        "$( locres libs/qt/$UVER_QT_VERSION )/$UCORE_OS/$(majorminorver $UVER_QT_VERSION)/gcc_64/lib"
      ]
    },
    "override": {
      "QT_QPA_PLATFORM_PLUGIN_PATH": [
        "$( locres apps/qbs/$UVER_QBS_VERSION )/bin/$UCORE_OS/$UCORE_OS_VERSION/lib/qbs/plugins",
        "$( locres libs/qt/$UVER_QT_VERSION )/$UCORE_OS/$(majorminorver $UVER_QT_VERSION)/gcc_64/plugins"
      ]
    }
  }
}
