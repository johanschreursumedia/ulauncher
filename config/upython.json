{
  "launcherType": "bin",
  "config": {
    "executable": "$UAPPS_ROOT/python/$UVER_UPYTHON_VERSION/bin/$UCORE_OS/$UCORE_OS_VERSION/bin/python$(majorminorver $UVER_UPYTHON_VERSION)"
  },
  "addons": {
    "pyside2": {
      "env": {
        "prepend": {
          "PYTHONPATH": [
            "$ULIBS_ROOT/pyside2/$UVER_PYSIDE2_VERSION/$UCORE_OS"
          ],
          "LD_LIBRARY_PATH": [
            "$ULIBS_ROOT/qt/$UVER_QT_VERSION/$UCORE_OS/$(majorminorver $UVER_QT_VERSION)/gcc_64/lib"
          ]
        },
        "override": {
          "QT_QPA_PLATFORM_PLUGIN_PATH": [
            "$ULIBS_ROOT/qt/$UVER_QT_VERSION/$UCORE_OS/$(majorminorver $UVER_QT_VERSION)/gcc_64/plugins"
          ]
        }
      }
    },
    "setuptools": {
      "env": {
        "prepend": {
          "PYTHONPATH": [
            "$ULIBS_ROOT/setuptools/$UVER_SETUPTOOLS_VERSION/lib/python$(majorminorver $UVER_UPYTHON_VERSION)/site-packages"
          ]
        }
      }
    },
    "openimageio": {
      "env": {
        "prepend": {
          "PYTHONPATH": "$ULIBS_ROOT/openimageio/$UVER_OPENIMAGEIO_VERSION/$UCORE_OS/lib/python$(majorver $UVER_UPYTHON_VERSION)/site-packages",
          "LD_LIBRARY_PATH": [
            "$ULIBS_ROOT/png/$UVER_PNG_VERSION/$UCORE_OS/lib",
            "$ULIBS_ROOT/openimageio/$UVER_OPENIMAGEIO_VERSION/$UCORE_OS/lib"
          ]
        }
      }
    },
    "opencolorio": {
      "env": {
        "prepend": {
          "PYTHONPATH": "$ULIBS_ROOT/opencolorio/$UVER_OPENCOLORIO_VERSION/$UCORE_OS/lib/python$(majorminorver $UVER_UPYTHON_VERSION)/site-packages",
          "LD_LIBRARY_PATH": [
            "$ULIBS_ROOT/opencolorio/$UVER_OPENCOLORIO_VERSION/$UCORE_OS/lib"
          ]
        },
        "override": {
          "OCIO": "$ULIBS_ROOT/opencolorio/configs/nuke-default/config.ocio"
        }
      }
    },
    "tank": {
      "env": {
        "prepend": {
          "PYTHONPATH": "$ULIBS_ROOT/tank/$UVER_TANK_VERSION/lib/python$(majorminorver $UVER_UPYTHON_VERSION)"
        }
      }
    }
  }
}
