{
  "launcherType": "bin",
  "config": {
    "executable": "$( locres apps/nuke/$UVER_NUKE_VERSION )/bin/$UCORE_OS/$UCORE_OS_VERSION/Nuke*"
  },
  "env": {
    "unset": [
      "QT_PLUGIN_PATH"
    ],
    "override": {
      "NUKE_TEMP_DIR": "$UMEDIA_TEMP_LOCAL_DIR"
    }
  },
  "addons": {
    "shotgun": {
      "env": {
        "prepend": {
          "PYTHONPATH": [
            "$( locres libs/shotgun/$UVER_SHOTGUN_VERSION )/lib/python2"
          ]
        }
      }
    },
    "ushotgun": {
      "env": {
        "prepend": {
          "PYTHONPATH": [
            "$( locres libs/ushotgun/$UVER_USHOTGUN_VERSION )/lib/python$(majorver $UVER_UPYTHON_VERSION)/lib"
          ]
        }
      }
    },
    "basetools": {
      "env": {
        "prepend" : {
          "PYTHONPATH": [
            "$( locres tools/base/$UVER_BASETOOLS_VERSION )/python/lib"
          ]
        }
      }
    },
    "nuketools": {
      "env" : {
        "prepend": {
          "NUKE_PATH": [
            "$( locres tools/nuke/$UVER_NUKETOOLS_VERSION )/$(majorver $UVER_NUKE_VERSION)/python/scripts"
          ],
          "PYTHONPATH": [
            "$( locres tools/nuke/$UVER_NUKETOOLS_VERSION )/$(majorver $UVER_NUKE_VERSION)/python/lib"
          ]
        },
        "override" : {
          "DEADLINE_PATH": "$( locres apps/deadline/$UVER_DEADLINECOMMAND_VERSION )/bin/$UCORE_OS/$UCORE_OS_VERSION/bin"
        }
      }
    },
    "neatvideo":{
      "env":{
        "prepend": {
          "OFX_PLUGIN_PATH": [
            "$( locres plugins/nuke/$(majorver $UVER_NUKE_VERSION)/neatVideo/$UVER_NEATVIDEO_VERSION )/bin/$UCORE_OS/$UCORE_OS_VERSION"
          ]
        }
      }
    },
    "ingestor":{
      "env": {
        "prepend": {
          "PYTHONPATH": [
            "$( locres apps/ingestor/$UVER_INGESTOR_VERSION )/python$(majorver $UVER_UPYTHON_VERSION)/lib"
          ]
        }
      }
    }
  }
}
