{
  "launcherType": "bin",
  "config": {
    "executable": "$UAPPS_ROOT/nuke/$UVER_NUKE_VERSION/bin/$UCORE_OS/$UCORE_OS_VERSION/Nuke*"
  },
  "env": {
    "unset": [
      "QT_PLUGIN_PATH"
    ],
    "override": {
      "NUKE_TEMP_DIR": "$UMEDIA_TEMP_LOCAL_DIR"
    }
  },
  "addons": {
    "neatvideo":{
      "env":{
        "prepend": {
          "OFX_PLUGIN_PATH": [
            "$UPLUGINS_ROOT/nuke/$(majorver $UVER_NUKE_VERSION)/neatVideo/$UVER_NEATVIDEO_VERSION/bin/$UCORE_OS/$UCORE_OS_VERSION"
          ]
        }
      }
    },
    "eddy":{
      "env":{
        "prepend": {
          "NUKE_PATH": [
            "$UPLUGINS_ROOT/nuke/$(majorver $UVER_NUKE_VERSION)/eddy/$UVER_EDDY_VERSION/bin/$UCORE_OS"
          ]
        }
      }
    }
  }
}
