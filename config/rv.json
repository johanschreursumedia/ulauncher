{
  "launcherType": "bin",
  "config": {
    "executable": "$( locres apps/rv/$UVER_RV_VERSION )/bin/$UCORE_OS/$UCORE_OS_VERSION/bin/rv"
  },
  "env": {
    "override": {
      "RV_SHOTGUN_DEFAULT_SERVER_URL": "$UMEDIA_SHOTGUN_URL",
      "RV_SHOTGUN_DEFAULT_LOGIN": "$USERNAME"
    },
    "unset": [
      "QT_PLUGIN_PATH"
    ]
  },
  "addons": {
    "basetools": {
      "env": {
        "prepend" : {
          "PYTHONPATH": [
            "$( locres tools/base/$UVER_BASETOOLS_VERSION )/python/lib"
          ]
        }
      }
    }
  }
}
